{
    "compilerOptions": {
        "outDir": "../public/dist/",
        "noImplicitAny": true,
        "strictNullChecks": true,
        "strict": true,
        "sourceMap": true,
        "module": "esnext",
        "target": "es2015",
        "jsx": "react-jsx",
        "allowJs": true,
        "moduleResolution": "node",
        "allowSyntheticDefaultImports": true,
        "plugins": [{ "name": "typescript-plugin-css-modules" }]
    }
}